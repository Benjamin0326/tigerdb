<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/head %>
  </head>
  <body>
      <div class="header">
      <% include ../partials/header %>
      </div>
    <div class="container" style="margin-top:30px">
      <div class="list-group">
        <div class="list-group-item active" style="height:50px"><h4><center>스마트폰 대여기록</center></h4></div>
        <% phonerents.forEach(function(result) { %>
          <div class="list-group-item">
              <p class="list-group-item-text col-md-1"><%= result[0] %></p>
              <p class="list-group-item-text col-md-1"><%= result[8] %></p>
              <p class="list-group-item-text col-md-2"><%= result[9] %></p>
              <p class="list-group-item-text col-md-4"><% if(result[6]==null){ %>대여 중(<%= result[5].toString().substr(0,16) %>~)<% }else{ %>반납 완료(<%= result[5].toString().substr(0,16) %>~<%= result[6].toString().substr(0,16) %>)<% }; %></p>
              <p class="list-group-item-text col-md-3">
              <% if(result[2]==null){ %> 없음
              <% }else{ %> <%= result[2] %> <% } %>
              </p>
              <br>
              <p class="list-group-item-text" style="text-algin:right; margin-left:700px"></p>
          </div>
          <% }); %>
      </div>
    </div>


    <div class="container" style="margin-top:30px">
      <div class="list-group">
        <div class="list-group-item active" style="height:50px"><h4><center>USIM 대여기록</center></h4></div>
        <% usimrents.forEach(function(result) { %>
          <div class="list-group-item">
            <p class="list-group-item-text col-md-1"><%= result[0] %></p>
            <p class="list-group-item-text col-md-1"><%= result[8] %></p>
            <p class="list-group-item-text col-md-2"><%= result[9] %></p>
            <p class="list-group-item-text col-md-4"><% if(result[6]==null){ %>대여 중(<%= result[5].toString().substr(0,16) %>~)<% }else{ %>반납 완료(<%= result[5].toString().substr(0,16) %>~<%= result[6].toString().substr(0,16) %>)<% }; %></p>
            <p class="list-group-item-text col-md-3">
            <% if(result[2]==null){ %> 없음
              <% }else{ %> <%= result[2] %> <% } %></p>
            <br>
            <p class="list-group-item-text" style="text-algin:right; margin-left:700px"></p>
          </div>
          <% }); %>
      </div>
    </div>

  </body>
</html>
