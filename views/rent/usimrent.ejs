<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/head %>
  </head>
  <body>
    <div class="header">
      <% include ../partials/header %>
    </div>
    <div class="container" style="margin-top:30px">
      <form role="form" id="body" method="post" action="/rent/usimrent/commit/<%= results[0][0] %>">
        <div class="form-group">
          <label for="InputName">USIM : <%= results[0][2] %>(<%= results[0][3] %>)</label>
          <label for="InputName"><%= results[0][1] %>(<%= results[0][4] %>)</label>
        </div>
        <div class="form-group">
          <label for="sel1">대여자</label>
          <select class="form-control" name="renter" id="sel1">
            <% results.forEach(function(result) { %>
              <option>(<%= result[5] %>)<%= result[6] %>(<%= result[7] %>)</option>
            <% }); %>
          </select>
        </div>
        <div class="form-group">
          <label for="InputMessage">설명</label>
          <input type="text" class="form-control" name="rentdescription" id="InputName" placeholder="Input Description">
        </div>
        <input type="submit" name="submit" id="submit" value="대여처리" class="btn btn-primary pull-right">
      </form>
    </div>
  </body>
</html>
