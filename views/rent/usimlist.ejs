<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/head %>
  </head>
  <body>
      <div class="header">
      <% include ../partials/header %>
      </div>

    <div class="container" style="margin-top:30px">
      <div class="list-group">
        <% if(results[0]==null){ %>
          <div class="list-group-item active"><h4>해당 USIM의 대여기록이 없습니다.</h4></div>
        <% }else{ %>
        <div class="list-group-item active"><h4>해당 Usim의 대여기록</h4></div>
        <% results.forEach(function(result) { %>
          <div class="list-group-item">
            <p class="list-group-item-text col-md-4"><%= result[10] %></p>
            <p class="list-group-item-text col-md-4"><%= result[8] %></p>
            <p class="list-group-item-text col-md-4"><%= result[2] %></p>
            <br>
            <p class="list-group-item-text" style="text-algin:right; margin-left:700px"><% if(result[6]==null){ %>대여 중(<%= result[5].toString().substr(0,16) %>~)<% }else{ %>반납 완료(<%= result[5].toString().substr(0,16) %>~<%= result[6].toString().substr(0,16) %>)<% }; %></p>
          </div>
          <% }); %>
          <% }; %>
      </div>
    </div>

  </body>
</html>
