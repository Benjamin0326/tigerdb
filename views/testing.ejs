<!DOCTYPE html>
<html>
  <head>
   <% include partials/head %>
  </head>

  <body>


            <% if(type==0) {%>



            <table width="80%" border="1px" align="center">
              <tr>
                <td align="center">#</td>
                <td>SUMMARY</td>
                <td>DES</td>
              </tr>
            <% bugs.forEach(function(bug) { %>
              <tr>
                <td><%= bug[0] %></td>
                <td><%= bug[1] %></td>
                <td><%= bug[2] %></td>
              </tr>
            <% } ); %>
            </table>

            <%} else if(type==1) { %>

            <table width="80%" border="1px" align="center">
              <tr align="center">
                <td >set#</td>
                <td>case#</td>
                <td>DES</td>
                <td>RESULT </td>
                <td>submit </td>
              </tr>
            <% manuals.forEach(function(manual) {%>
              <tr align="center">
                <td><%= manual[0] %></td>
                <td><%= manual[1] %></td>
                <td><%= manual[5] %>(ER : <% if(manual[7]==1){ %>PASS<% }else if(manual[7]==0){ %>FAIL<% }; %>)</td>
                <form action="/job/typeresult" method="GET">
                <td><input type="hidden" name = "setno" value="<%= manual[0]%>"/>
                    <input type="hidden" name = "caseno" value="<%= manual[1]%>"/>
                    <input type="hidden" name = "projnum" value="<%= manual[3]%>"/>
                    <input type="hidden" name = "testtype" value="<%= type%>"/>

                    <% if(manual[4]==0) {%>

                    <select id= "result" name="result" size="1">
                    <option value="" >선택</option>
                    <option value ="0" selected="selected">정상</option>
                    <option value ="1">이상</option></select></td>

                    <%} else if(manual[4]==1){%>
                    <select id= "result" name="result" size="1">
                    <option value="" >선택</option>
                    <option value ="0">정상</option>
                    <option value ="1" selected="selected">이상</option></select></td>
                    <%} else {%>
                    <select id= "result" name="result" size="1">
                    <option value="" selected="selected">선택</option>
                    <option value ="0" >정상</option>
                    <option value ="1">이상</option></select></td>
                    <%}%>

                <td><button type="submit">submit
                </button></td>
                </form>
              </tr>
            <% } ); %>
            </table>

            <% } %>


</body>

</html>
