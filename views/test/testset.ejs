<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/head %>
  </head>
  <body>
      <div class="header">
        <% include ../partials/header %>
      </div>
      <div class="container" style="margin-top:30px">
        <a href="/test/testset/add"><button class="btn btn-primary">Add TestSet</button></a>
        <div class="list-group" style="margin-top:10px">
          <% testsets.forEach(function(testset) { %>
            <div class="list-group-item">
              <div class="row">
                <a href="/test/testset/<%= testset[0] %>" style="color:black"><h4 class="list-group-item-text col-md-6"><%= testset[1] %></h4></a>
                <p class="list-group-item-text col-md-3" style="margin-top:10px">Project : <%= testset[2] %></p>
                <p class="list-group-item-text col-md-3" style="margin-top:10px">Tester : <%= testset[3] %></p>
              </div>
            </div>
            <% }); %>
        </div>

      </div>

  </body>
</html>
