<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/head %>
  </head>
  <body>
    <div class="header">
      <% include ../partials/header %>
    </div>
    <div class="container" style="margin-top:30px">
      <a href="/people/prize/totallist"><button class="btn btn-primary">전체 포상 기록</button></a>
    </div>

    <div class="container" style="margin-top:30px">
      <div class="list-group">
        <div class="list-group-item active"><h4>사원</h4></div>
        <% peoples.forEach(function(people) { %>
          <div class="list-group-item" style="height:60px;">
            <p class="list-group-item-text col-md-3"><a href="/people/prizelist/<%= people[0] %>"><%= people[1] %></a></p>
            <p class="list-group-item-text col-md-3"><%= people[2] %></p>
            <p class="list-group-item-text col-md-4"><%= people[6] %></p>
            <p class="list-group-item-text col-md-2">
            <a href="/people/prize/award/<%= people[0] %>"><button class="btn btn-primary">포상</button></a>
            </p>
          </div>
          <% }); %>
      </div>
    </div>
  </body>
</html>
