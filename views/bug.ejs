
<!DOCTYPE html>
<html>
  <head>
    <% include partials/head %>
  </head>
  <body>
    <div class="header">
      <% include partials/header %>
    </div>
    <div class="container" style="margin-top:30px">
      <a href="/bug/report"><button class="btn btn-primary">Report Bug</button></a>
      <div class="list-group" style="margin-top:10px">
        <% bugs.forEach(function(bug) { %>
          <div class="list-group-item">
            <h4 class="list-group-item-text"><a href="bug/<%= bug[0] %>" style="color:black"><%= bug[1] %></a></h4>
            <br>
            <p class="list-group-item-text">Status = <%= bug[6] %> / Type = <%= bug[7] %></p>
            <p class="list-group-item-text" style="text-algin:right; margin-left:700px"><%= bug[4].toString().substr(0,16) %></p>
          </div>
          <% }); %>
      </div>
    </div>





  </body>
</html>
